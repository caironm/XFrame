{
  "name": "caironm/xframe",
  "description": "Um CMS de fácil aplicação",
  "keywords": [
    "xframe",
    "Cairon M"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Philipe Cairon M.",
      "email": "caironm.go@gmail.com"
    }
  ],
  "support": {
    "email": "contato@stdooceano.com"
  },
  "require": {
    "php": "^7.3",
    "catfan/medoo": "1.*",
    "filp/whoops": "2.*",
    "mikecao/flight": "1.3.*",
    "twig/twig": "1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.4.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Src\\": "src/",
      "Config\\": "config/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Testes\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
